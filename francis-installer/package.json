{
  "name": "francis-setup",
  "version": "1.0.0",
  "description": "Francis Extension Auto-Installer - 1-click installation for Chrome",
  "main": "install.js",
  "scripts": {
    "start": "node install.js",
    "build": "pkg install.js --target node18-win-x64 --output FrancisSetup.exe",
    "build-mac": "pkg install.js --target node18-macos-x64 --output FrancisSetup-Mac",
    "build-linux": "pkg install.js --target node18-linux-x64 --output FrancisSetup-Linux"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "fs-extra": "^11.0.0",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "puppeteer": "^21.0.0"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  },
  "pkg": {
    "assets": [
      "francis-extension/**/*"
    ],
    "outputPath": "./"
  },
  "author": "Fiscal.ia",
  "license": "MIT"
}
